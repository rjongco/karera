{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "node ./dist/server/server.js",
    "deploy": "npm run build && npm start",
    "build": "tsc",
    "dev": "nodemon",
    "preInitDev": "kill $(lsof -t -i:8001) & npm run dropDb && npm run dev",
    "dropDb": "mysql -uroot -pKareraDev69420 -e 'DROP DATABASE IF EXISTS zodiacDev; CREATE DATABASE zodiacDev;'",
    "postInitDev": "npm run migrate && sequelize db:migrate && sequelize db:seed:all",
    "test": "jest",
    "migrate": "sequelize-cli db:migrate --env development --migrations-path ./custom_migrations",
    "migrate:generate-user": "sequelize migration:generate --name create_users_table --migrations-path ./custom_migrations",
    "migrate:generate-logs": "sequelize migration:generate --name create_logs_table --migrations-path ./custom_migrations",
    "migrate:generate-sessions": "sequelize migration:generate --name create_sessions_table --migrations-path ./custom_migrations",
    "migrate:generate-notifications": "sequelize migration:generate --name create_notifications_table --migrations-path ./custom_migrations",
    "migrate:generate-userGroup": "sequelize migration:generate --name create_user-groups_table --migrations-path ./custom_migrations",
    "migrate:generate-addresses": "sequelize migration:generate --name create_addresses_table --migrations-path ./custom_migrations",
    "migrate:generate-provinces": "sequelize migration:generate --name create_provinces_table --migrations-path ./custom_migrations",
    "migrate:generate-cities": "sequelize migration:generate --name create_cities_table --migrations-path ./custom_migrations",
    "migrate:generate-barangays": "sequelize migration:generate --name create_barangays_table --migrations-path ./custom_migrations",
    "migrate:generate-referrals": "sequelize migration:generate --name create_referrals_table --migrations-path ./custom_migrations",
    "migrate:generate-gameList": "sequelize migration:generate --name create_games_list_table --migrations-path ./custom_migrations",
    "migrate:generate-gameAssets": "sequelize migration:generate --name create_game_assets_table --migrations-path ./custom_migrations",
    "migrate:generate-bettings": "sequelize migration:generate --name create_bettings_table --migrations-path ./custom_migrations",
    "migrate:generate-gameMatches": "sequelize migration:generate --name create_game_matches_table --migrations-path ./custom_migrations",
    "migrate:generate-gameMatchBettors": "sequelize migration:generate --name create_game_match_bettors_table --migrations-path ./custom_migrations",
    "migrate:generate-gameMatchBettings": "sequelize migration:generate --name create_game_match_bettings_table --migrations-path ./custom_migrations",
    "migrate:generate-cards": "sequelize migration:generate --name create_cards_table --migrations-path ./custom_migrations",
    "migrate:generate-seeder-provinces": "sequelize migration:generate --name seedProvinces --migrations-path ./seeders",
    "migrate:generate-seeder-cities": "sequelize migration:generate --name seedCities --migrations-path ./seeders",
    "migrate:generate-seeder-barangays": "sequelize migration:generate --name seedBarangays --migrations-path ./seeders",
    "migrate:generate-seeder-gameList": "sequelize migration:generate --name seedGameList --migrations-path ./seeders",
    "migrate:generate-seeder-gameAssets": "sequelize migration:generate --name seedGameAssets --migrations-path ./seeders",
    "seed:user": "sequelize-cli db:seed --env development --seed ./seeders/20240413053605-userSeeder.js",
    "seed:provinces": "sequelize-cli db:seed --env development --seed ./seeders/20240425044605-seedProvinces.js",
    "seed:cities": "sequelize-cli db:seed --env development --seed ./seeders/20240425045750-seedCities.js",
    "seed:barangays": "sequelize-cli db:seed --env development --seed ./seeders/20240425072224-seedBarangays.js",
    "seed:gameList": "sequelize-cli db:seed --env development --seed ./seeders/20240523105842-seedGamesList.js",
    "seed:gameAssets": "sequelize-cli db:seed --env development --seed ./seeders/20240515025106-seedGameAssets.js",
    "seed:all": "npm run seed:provinces && npm run seed:cities && npm run seed:barangays && npm run seed:gameList && npm run seed:gameAssets"
  },
  "nodemonConfig": {
    "watch": [
      "../server"
    ],
    "ext": "ts",
    "exec": "ts-node server.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "jest": {
    "preset": "ts-jest",
    "testMatch": [
      "<rootDir>/**/*.test.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "testTimeout": 5000
  },
  "devDependencies": {
    "@jest/types": "^29.6.3",
    "@types/babel__core": "^7.20.5",
    "@types/bluebird": "^3.5.42",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.12.7",
    "@types/puppeteer": "^7.0.4",
    "@types/sequelize": "^4.28.20",
    "eslint": "^8.57.0",
    "exceljs": "^4.4.0",
    "fastify": "^4.26.2",
    "jest": "^29.7.0",
    "jest-dev-server": "^10.0.0",
    "jest-puppeteer": "^10.0.1",
    "puppeteer": "^22.8.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fastify/auth": "^4.6.1",
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.0",
    "@fastify/multipart": "^6.0.0",
    "@fastify/session": "^10.7.0",
    "@fastify/static": "^7.0.3",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "@fastify/view": "^9.0.0",
    "@fastify/websocket": "^10.0.1",
    "@mui/material": "^5.15.15",
    "argon2": "^0.40.3",
    "axios": "^1.6.8",
    "bitecs": "^0.3.40",
    "crypto": "^1.0.1",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "env-cmd": "^10.1.0",
    "fastify-multipart": "^5.4.0",
    "fastify-plugin": "^4.5.1",
    "fastify-user-agent": "^1.0.1",
    "fs": "^0.0.1-security",
    "https": "^1.0.0",
    "mkdirp": "^3.0.1",
    "moment-timezone": "^0.5.45",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.9.4",
    "nodemon": "^3.1.0",
    "react-avatar-editor": "^13.0.2",
    "sequelize": "^6.37.2",
    "sequelize-cli": "^6.6.2",
    "socket.io": "^4.7.5",
    "stream": "^0.0.2",
    "uecs": "^0.4.2",
    "util": "^0.12.5",
    "uuid": "^9.0.1",
    "ws": "^8.16.0"
  }
}
